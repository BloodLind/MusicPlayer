<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:MusicPlayer.WPF">


    <ImageSource x:Key="Add">../themes/drawable/dark/add.png</ImageSource>
    <ImageSource x:Key="Play">../themes/drawable/dark/playbutton.png</ImageSource>
    <ImageSource x:Key="Pause">../themes/drawable/dark/pausebutton.png</ImageSource>
    <ImageSource x:Key="Next">../themes/drawable/dark/nextbutton.png</ImageSource>
    <ImageSource x:Key="Previous">../themes/drawable/dark/previousbutton.png</ImageSource>
    <ImageSource x:Key="Shuffle">../themes/drawable/dark/shuffle.png</ImageSource>
    <ImageSource x:Key="SoundWave">../themes/drawable/dark/soundwave.png</ImageSource>
    <ImageSource x:Key="Waves">../themes/drawable/dark/waves.png</ImageSource>
    <ImageSource x:Key="Cancel">../themes/drawable/dark/cancel.png</ImageSource>
    <ImageSource x:Key="Other">../themes/drawable/dark/othersbutton.png</ImageSource>
    <ImageSource x:Key="Equalizer">../themes/drawable/dark/equalizer.png</ImageSource>
    <ImageSource x:Key="Library">../themes/drawable/dark/librarybutton.png</ImageSource>
    <ImageSource x:Key="More">../themes/drawable/dark/more.png</ImageSource>
    <ImageSource x:Key="MoreHorizontal">../themes/drawable/dark/morehorizontal.png</ImageSource>
    <ImageSource x:Key="Folder">../themes/drawable/dark/folder.png</ImageSource>
    <ImageSource x:Key="Picture">../themes/drawable/dark/picture.png</ImageSource>
    <ImageSource x:Key="Information">../themes/drawable/dark/information.png</ImageSource>
    <ImageSource x:Key="Lyrics">../themes/drawable/dark/songlyrics.png</ImageSource>
    <ImageSource x:Key="LyricsButton">../themes/drawable/dark/lyrics.png</ImageSource>
    <ImageSource x:Key="SoundBars">../themes/drawable/dark/soundbars.png</ImageSource>
    <ImageSource x:Key="Delete">../themes/drawable/dark/delete.png</ImageSource>
    <ImageSource x:Key="Speaker">../themes/drawable/dark/speaker.png</ImageSource>
    <ImageSource x:Key="Loop">../themes/drawable/dark/loopIcon.png</ImageSource>
    <ImageSource x:Key="Back">../themes/drawable/dark/previous.png</ImageSource>
    <ImageSource x:Key="Cover">../themes/drawable/dark/defaultcover.png</ImageSource>
    <ImageSource x:Key="Playlists">../themes/drawable/dark/playlist.png</ImageSource>
    <ImageSource x:Key="Queue">../themes/drawable/dark/queue.png</ImageSource>
    <ImageBrush
        x:Key="Background"
        ImageSource="../themes/drawable/backs/backdark1.png"
        Stretch="UniformToFill" />

    <SolidColorBrush x:Key="PrimaryDarkColor" Color="#0A0A0A" />
    <SolidColorBrush x:Key="PrimaryMidColor">
        <SolidColorBrush.Color>
            #101010
        </SolidColorBrush.Color>
    </SolidColorBrush>
    <SolidColorBrush x:Key="PrimaryLightColor" Color="#1C1C1C" />

    <SolidColorBrush x:Key="SecondaryDarkColor" Color="#FF131313" />
    <SolidColorBrush x:Key="SecondaryMidColor" Color="#FF131313" />
    <SolidColorBrush x:Key="SecondaryLightColor" Color="#FF131313" />

    <SolidColorBrush x:Key="ForegroundDarkColor" Color="#656565" />
    <SolidColorBrush x:Key="ForegroundMidColor" Color="#C6C6C6" />
    <SolidColorBrush x:Key="ForegroundLightColor" Color="#FFFFFF" />

    <SolidColorBrush x:Key="PreampEmptyColor" Color="#928E8E" />
    <SolidColorBrush x:Key="PreampFullColor" Color="#00D3FF" />
    <SolidColorBrush x:Key="EqualizerFullColor" Color="#009A1F" />
    <SolidColorBrush x:Key="EqualizerEmptyColor" Color="#928E8E" />


    <SolidColorBrush x:Key="RadialSliderBroderColor" Color="Transparent" />
    <Thickness x:Key="RadialSliderBorderThickness">0</Thickness>
    <SolidColorBrush x:Key="RadialSliderThumbColor" Color="#FFFFFF" />
    <SolidColorBrush x:Key="RadialSliderColor" Color="#2B2B2B" />

    <Style x:Key="Font" TargetType="{x:Type Window}">
        <Setter Property="FontFamily" Value="Yu Gothic UI" />
    </Style>



</ResourceDictionary>